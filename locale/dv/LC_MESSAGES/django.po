msgid "Home"
msgstr "ގޭ"

msgid "Stories"
msgstr "ކަހާޅާ"

msgid "Loading stories..."
msgstr "ކަހާޅާ ލޯޑުކުރަމުން..."

msgid "Welcome to VAAHAKAINN."
msgstr "ވާހަކައިން އަށް މަރުހަބާ."

msgid "Discover a wonderful collection of stories and exciting tales that take you on journeys through different worlds full of adventures and imagination"
msgstr "އެކްއެކް ދުނިޔެއްގައި އެޑްވެންޗަރާއި تصور ގެ ދުނިޔަކަށް ގެންދާ ފަރުވާ ކަހާޅާ އާއި ފުހުލްކަ ހަދިޔާއެއް ފުންސަންތާ"

msgid "Scroll to explore"
msgstr "ދުރުކަން ކުރުމަށް ސްކުރޯލް ކުރޭ"

msgid "Featured"
msgstr "ފީޗާ ކުރުވާފައިވާ"

msgid "Handpicked tales that will take you on unforgettable journeys"
msgstr "ހަތްހަވާނު ޞާރިކެއްގެ އަހާހީއް ފަތީހެކީ ގެ ކަހާޅާ"

msgid "Episodes"
msgstr "ޖަލްސާތަކެއް"

msgid "Dive into this captivating story filled with adventure and wonder."
msgstr "އެޑްވެންޗަރާއި ޔާކަލީސް ވާހަކައަކަށް ރައްދުވާ"

msgid "Explore Story"
msgstr "ވާހަކަ ހޯއް"

msgid "Stories Coming Soon"
msgstr "ކަހާޅާ އަވަސް"

msgid "We're crafting amazing tales that will transport you to magical worlds. Stay tuned for incredible adventures!"
msgstr "އަޅު ސިއުރީޔު ދުނިޔެއް ގައި ގެންދާ ހާސްކަހާޅާ މަސައްކަތް ކުރައާ އަވަސްވެކަން!"